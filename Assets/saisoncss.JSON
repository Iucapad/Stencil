{
  "startDocument": "@keyframes colorchange {from {background-color: var(--color-fill);}to {background-color: var(--accent-color);}}:root{--color-fill:#DDDDDD;--color-panel:#FAFAFA;--color-focus:#151545;--color-text:#000000;--color-grey:#5C575A;--backdrop-fill:#FFFFFF7F;",
  "accentColor": "--accent-color:#4169E1;--accent-soft:#4169E166;}",
  "basicStyle":"body{font-family:system-ui,-apple-system,BlinkMacSystemFont,'Segoe UI',Roboto,Ubuntu,'Helvetica Neue',sans-serif;background:var(--color-fill);color:var(--color-text);}\nh1{margin:1vmin 2vw;}\nh2{margin:0 0 10px 3.5vw;}\np{line-height:150%;margin:0 5vw;}",
  "inputs": "input,button{background:var(--color-panel);padding:5px 10px;border:none;border-radius:5px;outline:0 !important;font-size:1rem;color:var(--color-text);margin:5px;}button{background:var(--accent-soft);border-radius:10px;font-weight:bold;color:white;}\nbutton:focus{background:var(--accent-color);}\n.form{display:block;text-align:center;width:fit-content;margin:5vh auto;}\n.form span{display:block;text-align:left;font-weight:500;}",
  "darkMode": "@media(prefers-color-scheme: dark){:root{--color-fill:#151515;--color-panel:#070707;--color-text:#FFFFFF;--color-grey:#5C575A;--backdrop-fill:#0000007F;}}",
  "uiTabs": ".tab{position:absolute;display:flex;flex-flow:row;list-style-type:none;user-select:none;overflow:hidden;padding:0;margin:0;}\n.edge>.tabNavigation{content:url(../images/assets/navigation.svg);display:block;width:40px;height:40px;background:var(--accent-soft);}\n.tab>.item{outline:none;float:left;white-space:nowrap;}\n.tab.edge{position:absolute;top:0;flex-flow:column;width:40px;transition:width ease-in-out 0.3s;box-shadow:0 0 25px black;}\n.edge>.tab>.item{float:none;text-align:left;position:relative;transition:left 0.5s;}",
  "uiTabs_0": ".tab>.item{display:inline-block;text-align:center;font-size:1.25em;padding:14px 2px 2px 2px;margin:0 14px;color:var(--color-text);text-decoration:none;border-bottom:3px solid transparent;}\n.tab>.item:hover:not(#active){border-bottom:1.5px solid var(--accent-color);}\n.tab>#active{color:var(--accent-color);outline:none;transition:border-width 0.05s linear;border-bottom:3px solid var(--accent-color);}\n.tab>.item:focus-visible{text-shadow:0 0 1px var(--accent-soft);border-bottom:2px solid var(--color-focus);}\n.edge>.tab>.item{margin:3px 8vw 3px 0;padding:4px 2px 4px 8px;border-bottom:none;border-left:3px solid transparent;}\n.edge>.tab>.item:hover,.edge>#active{border-bottom:none;border-left:3px solid var(--accent-color);}",
  "uiTabs_1": ".tab>.item{padding:2px 12px 6px 12px;border-radius:10px;background:var(--color-panel);font-size:1.5em;color:var(--accent-color);margin:6px 2vw;;text-decoration:none;}\n.tab>.item:hover,.tab>#active{background: var(--accent-color);box-shadow:0px 0px 5px var(--accent-color);color:white;animation:colorchange 0.5s;text-decoration:none;}\n.edge>.tab>.item{padding:1px 8px 3px 8px;margin:5px 2vw;}",
  "uiActionButtons":".actionButton{background-color:#AAAAAA30 !important;color:#AA0000;text-align:center !important;border-radius:3px;margin:0;width:35px;height:35px;}\n.actionButton:hover{background-color:#AAAAAA45 !important;border:2px solid #CCCCCC;}",
  "uiComparator": ".comp_container{position:relative;min-height:100px;width:auto;border-radius:10px;background:var(--color-panel);box-shadow:0 0 5px black;overflow: hidden;transition:height 0.5s;}\n.comp_separator{cursor:e-resize;height:100%;top:50%;left:50%;position:absolute;background-color:white;z-index:99;}\n.separator_icon{background-color:var(--accent-color);height:50px;width:50px;position:absolute;left:-25px;top:-25px;border-radius:25px;opacity:0.5;transition:opacity 400ms;}\n.separator_icon:hover{opacity:1;}\n.comp_obj{width:100%;top:0;overflow:hidden;position:absolute;}\n.comp_obj>img{width:inherit;}\n.comp_overlay{z-index:98;background-color:var(--color-fill);}\n.comp_global{user-select:none;width:90%;max-width:500px;border-radius:10px;background:var(--accent-soft);}\n.comp_caption{text-align:justify;padding:5px 10px;}",
  "uiCards":".uiCard{border-radius: 15px;background-color:var(--backdrop-fill);backdrop-filter:blur(10px);-webkit-backdrop-filter:blur(10px);margin:10px;padding:10px;max-height:0;overflow:hidden;box-shadow:0 0 6px var(--color-grey);}",
  "uiSlideviews" : ".slideView{margin:0 auto;max-width:600px;}\n.slideView div{transition:opacity 0.5s,max-height 0.5s ease-out;}\n.slideView>div{text-align:center;}\n.slideView>div h1,h2,p{text-align:left;}",
  "uiSettingsList":".settingsList{width:100%;background:var(--color-panel);border-radius:6px;margin:10px 0;}\n.settingsList tr{line-height:35px;}\n.settingsList tr td{text-align:right;}\n.settingsList tr td:first-child{text-align:left;padding-left:5%;font-weight:100;}\n.settingsList tr td:last-child>*{text-align: right;}",
  "experimental": ""
}